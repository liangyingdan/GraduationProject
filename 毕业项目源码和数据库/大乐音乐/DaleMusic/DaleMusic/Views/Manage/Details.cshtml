
@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutManage.cshtml";
}
@using DaleMusic.Models;
@model UserInfo

    <form action="/Manage/Details" method="post" class="form" onSubmit="return check(this)">
        <input type="hidden" name="UserID" id="UserID" value="@Model.UserID" />
        <div><span>姓名：</span><input type="text" name="UserName" id="UserName" readonly="readonly" value="@Model.UserName" /><br /></div>
        <div><span>电话：</span><input type="text" name="UserPhone" id="UserPhone" readonly="readonly" value="@Model.UserPhone" /><br /></div>
        <div><span>密码：</span><input type="text" name="UserPwd" id="UserPwd" readonly="readonly" value="@Model.UserPwd" /><br /></div>
        <div><span>邮箱：</span><input type="text" name="UserEmail" id="UserEmail" readonly="readonly" value="@Model.UserEmail" /><br /></div>
        <a href="/Manage/Index">返回上一级</a>
    </form>


<div style="text-align:center;margin:50px 0; font:normal 14px/24px 'MicroSoft YaHei';">
</div>



